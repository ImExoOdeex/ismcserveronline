		"@types/loadable__server": "^5.12.6",
		"@types/morgan": "^1.9.4",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"dotenv": "^16.0.3",
		"eslint": "^8.27.0",
