{
  "private": true,
  "sideEffects": false,
  "version": "1.3.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc --skipLibCheck server.ts --outDir ./build --esModuleInterop && remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:build-server": "cross-env NODE_ENV=development tsc --skipLibCheck server.ts --outDir ./build --esModuleInterop",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./build/server.js --watch ./build/server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc -b",
    "theme": "chakra-cli tokens app/components/utils/theme.ts",
    "theme:watch": "chakra-cli tokens app/components/utils/theme.ts --watch"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.13",
    "@chakra-ui/react": "^2.4.3",
    "@chakra-ui/react-use-size": "^2.0.9",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@loadable/component": "^5.15.3",
    "@loadable/server": "^5.15.3",
    "@prisma/client": "^4.7.1",
    "@remix-run/serve": "^1.14.0",
    "framer-motion": "^7.6.19",
    "color": "^4.2.3",
    "fast-average-color": "^9.3.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "remix": "^1.14.0",
    "remix-auth-discord": "^1.2.1",
    "remix-utils": "^5.1.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@loadable/babel-plugin": "^5.15.3",
    "@loadable/webpack-plugin": "^5.15.2",
    "@remix-run/dev": "^1.14.0",
    "@remix-run/eslint-config": "^1.14.0",
    "@remix-run/express": "^1.14.0",
    "@remix-run/node": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.7.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}