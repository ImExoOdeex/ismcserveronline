{
	"private": true,
	"sideEffects": false,
	"version": "1.6.0",
	"scripts": {
		"build": "cross-env NODE_ENV=production tsc --skipLibCheck honoserver.ts --outDir ./build --esModuleInterop && remix build",
		"dev:hono": "cross-env NODE_ENV=development tsc --skipLibCheck ./honoserver.ts --outDir ./build --esModuleInterop && remix dev -c \"node ./build/honoserver.js\"",
		"dev": "cross-env NODE_ENV=development tsc --skipLibCheck ./server.ts --outDir ./build --esModuleInterop && remix dev -c \"node ./build/server.js\"",
		"start": "cross-env NODE_ENV=production node ./build/honoserver.js",
		"typecheck": "tsc -b",
		"theme": "chakra-cli tokens app/components/utils/theme.ts",
		"theme:watch": "chakra-cli tokens app/components/utils/theme.ts --watch"
	},
	"dependencies": {
		"@balavishnuvj/remix-seo": "^1.0.2",
		"@chakra-ui/icons": "^2.0.13",
